<?xml version="1.0" encoding="utf-8"?>
<Parameters FOVx="224" FOVy="176" FOVz="248" Name="P" Nx="448" Ny="176" Nz="496" TE="8"
            TR="50">
   <ConcatSequence Name="PE" Observe="NX=P.Nx" Repetitions="NX">
      <ConcatSequence Name="C" Observe="NY=P.Ny" Repetitions="NY">
         <ATOMICSEQUENCE Name="A1">
            <HARDRFPULSE Axis="RF" Duration="0.1" FlipAngle="20" InitialPhase="C*(C+1)*50" Name="P1"
                         Observe="C=C.Counter"/>
         </ATOMICSEQUENCE>
         <ATOMICSEQUENCE Name="A2">
            <TRAPGRADPULSE Area="-KMX+PE*DKX" Axis="GX" Name="P2"
                           Observe="KMX=P.KMAXx, PE=PE.Counter, DKX=P.DKx"/>
            <TRAPGRADPULSE Area="-KMY+C*DKY" Axis="GY" Name="P3"
                           Observe="KMY=P.KMAXy, C=C.Counter, DKY=P.DKy"/>
         </ATOMICSEQUENCE>
         <ATOMICSEQUENCE Name="A3">
            <TRAPGRADPULSE ADCs="NZ" Axis="GZ" FlatTopArea="2*KMZ" FlatTopTime="4" Name="P4"
                           Observe="KMZ=P.KMAXz, NZ=P.Nz"
                           PhaseLock="1"/>
         </ATOMICSEQUENCE>
         <ATOMICSEQUENCE Name="A4">
            <TRAPGRADPULSE Area="1.5*A" Axis="GX" Name="P6" Observe="A=P4.Area"/>
            <TRAPGRADPULSE Area="-A" Axis="GY" Name="P7" Observe="A=P3.Area"/>
         </ATOMICSEQUENCE>
         <DELAYATOMICSEQUENCE Delay="TR" DelayType="B2E" Name="D2" Observe="TR=P.TR" StartSeq="A1"/>
      </ConcatSequence>
   </ConcatSequence>
</Parameters>